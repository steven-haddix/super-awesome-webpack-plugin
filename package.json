{
  "name": "super-awesome-webpack-plugin",
  "title": "Super Awesome Webpack Plugin",
  "version": "1.0.1",
  "description": "An opinionated Webpack plugin for generating static websites using React and Redux.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:publish": "npm run build && npm publish",
    "cover": "nyc report --reporter=lcov --reporter=text-lcov | coveralls",
    "test": "cross-env NODE_ENV=test nyc babel-tape-runner tests/**/*.test.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steven-haddix/super-awesome-webpack-plugin.git"
  },
  "keywords": [
    "Static Website Generaton",
    "Webpack",
    "React",
    "Redux"
  ],
  "author": "Steven Haddix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/steven-haddix/super-awesome-webpack-plugin/issues"
  },
  "homepage": "https://github.com/steven-haddix/super-awesome-webpack-plugin#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^1.0.0",
    "coveralls": "^2.11.14",
    "cross-env": "^3.1.3",
    "nyc": "^8.4.0",
    "proxyquire": "^1.7.10",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "webpack": "^1.13.3",
    "webpack-sources": "^0.1.2"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "extension": [
      ".js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
