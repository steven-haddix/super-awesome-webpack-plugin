{
  "name": "super-awesome-webpack-plugin",
  "title": "Super Awesome Webpack Plugin",
  "version": "2.0.14",
  "description": "A slightly opinionated Webpack plugin for generating static websites using React, React Router, and Redux.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:publish": "npm run build && npm version patch && npm publish",
    "cover": "nyc report --reporter=lcov --reporter=text-lcov | coveralls",
    "test": "cross-env NODE_ENV=test nyc tape tests/**/*.test.js | tap-spec",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/steven-haddix/super-awesome-webpack-plugin.git"
  },
  "keywords": [
    "Static Website Generaton",
    "Webpack",
    "React",
    "Redux"
  ],
  "author": "Steven Haddix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/steven-haddix/super-awesome-webpack-plugin/issues"
  },
  "homepage": "https://github.com/steven-haddix/super-awesome-webpack-plugin#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-register": "^6.18.0",
    "clean-webpack-plugin": "^0.1.14",
    "cross-env": "^3.1.3",
    "lodash": "^4.17.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "uuid": "^3.0.0",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1",
    "webpack-sources": "^0.1.2"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "extension": [
      ".js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^1.0.0",
    "coveralls": "^2.11.14",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "isomorphic-style-loader": "^1.1.0",
    "node-sass": "^3.13.0",
    "nyc": "^8.4.0",
    "proxyquire": "^1.7.10",
    "sass-loader": "^4.0.2",
    "semantic-release": "^6.3.2",
    "style-loader": "^0.13.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "url-loader": "^0.5.7"
  }
}
